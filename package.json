{
  "name": "cline-exporter",
  "version": "0.1.0",
  "description": "Convert Cline conversation history into a self-contained static website",
  "type": "module",
  "bin": {
    "cline-exporter": "./dist/cli.js",
    "cline-export": "./dist/cli.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "cline-exporter": "tsup --silent && node dist/cli.js",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "cline",
    "exporter",
    "conversation",
    "static-site"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/masto/cline-exporter.git"
  },
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "highlight.js": "^11.11.1",
    "marked": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "jiti": "^2.6.1",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  }
}
